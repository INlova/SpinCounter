+b('section').page.--upload#page--upload
  +b('form').upload-form.form#file-form
    +e('label').label Choose a directory to upload
      +e('svg').icon(width="100", height="100")
        use(xlink:href="#download")
      +e('input').input(type="file" id="file-input" enctype="multipart/form-data" webkitdirectory directory multiple="multiple")
    +e('p').error.hidden There are no valid files in chosen directory
